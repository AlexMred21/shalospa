{
    "version": 2,
    "name": "museum-app-full-stack-capstone-react",
    "routes": [
        {
            "src": "^/static/(.*)",
            "dest": "/static/$1"
          },
        { 
            "src": "/.*", 
            "dest": "/index.html" 
        },
        {
            "src": "^/favicon.ico$",
            "dest": "/favicon.ico"
        }
    ]
  }